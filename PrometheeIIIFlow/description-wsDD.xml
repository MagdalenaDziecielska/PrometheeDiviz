<?xml version="1.0" encoding="iso-8859-1"?>
<program_description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../docs/w3-ws/specificationData/description.xsd">
  <program provider="PUT" name="PrometheeIIIFlow" displayName="PrometheeIIIFlow" version="0.0.1" />
  <documentation>
    <description>
    	Final outranking relations in Promethee III.
    </description>
    <url>https://github.com/MagdalenaDziecielska/PrometheeIIIFlow</url>
  </documentation>
  <parameters>

    <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
      <documentation>
        <description>Alternatives which will be considered.</description>
      </documentation>
      <xmcda tag="alternatives" />
    </input>
    
    <input id="input2" name="positive_flows" displayName="positive_flows" isoptional="0">
      <documentation>
        <description>Positive outranking flows.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>
	
	<input id="input3" name="negative_flows" displayName="negative_flows" isoptional="0">
      <documentation>
        <description>Negative outranking flows.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>
	
	<input id="input4" name="preferences" displayName="preferences" isoptional="0">
      <documentation>
        <description>Aggregated preferences matrix.</description>
      </documentation>
      <xmcda tag="alternativesMatrix" />
    </input>
	
	<input id="input5" name="method_parameters" displayName="method_parameters" isoptional="0">
		<documentation>
			<description>Parameter alpha required in Promethee III flows calculation.</description>
		</documentation>
		<xmcda tag="programParameters">
			<![CDATA[
				<programParameters>
					<parameter id="alpha">
						<values>
							<value>
								<real>%1</real>
							</value>
						</values>
					</parameter>
				</programParameters>
			]]>
		</xmcda>
		<gui status="preferGUI">
			<entry id="%1" type="real" displayName="alpha">
				<documentation>
					<description>An alpha parameter.</description>
				</documentation>
				<constraint>
				  <description>This identifier cannot be empty</description>
				  <code>! %1.isEmpty()</code>
				</constraint>
				<defaultValue>0.0</defaultValue>
			</entry>
		</gui>
	</input>

    <output id="output1" name="ranking" displayName="ranking">
      <documentation>
        <description>Result computed from the given data.</description>
      </documentation>
      <xmcda tag="alternativesMatrix" />
    </output>
    
    <output id="output2" name="messages" displayName="messages">
      <documentation>
        <description>Messages or errors generated by this module.</description>
      </documentation>
      <xmcda tag="programExecutionResult" />
    </output>

  </parameters>
</program_description>