<?xml version="1.0" encoding="iso-8859-1"?>
<program_description xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../docs/w3-ws/specificationData/description.xsd">
  <program provider="PUT" name="PrometheeIRanking" displayName="PrometheeIRanking" version="1.0.0" />
  <documentation>
    <description>
    	Outranking relations in Promethee I.
    </description>
    <contact><![CDATA[Magdalena Dzi?cielska <magdalenadziecielska6@gmail.com>]]></contact>
    <url>https://github.com/MagdalenaDziecielska/PrometheeDiviz</url>
  </documentation>
  <parameters>

    <input id="input1" name="alternatives" displayName="alternatives" isoptional="0">
      <documentation>
        <description>Alternatives which will be considered.</description>
      </documentation>
      <xmcda tag="alternatives" />
    </input>

    <input id="input2" name="positive_flow" displayName="positive_flows" isoptional="0">
      <documentation>
        <description>Positive outranking flows.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

	<input id="input3" name="negative_flow" displayName="negative_flows" isoptional="0">
      <documentation>
        <description>Negative outranking flows.</description>
      </documentation>
      <xmcda tag="alternativesValues" />
    </input>

    <output id="output1" name="ranking" displayName="ranking">
      <documentation>
        <description>Result computed from the given data.</description>
      </documentation>
      <xmcda tag="alternativesMatrix" />
    </output>

    <output id="output2" name="messages" displayName="messages">
      <documentation>
        <description>Messages or errors generated by this module.</description>
      </documentation>
      <xmcda tag="programExecutionResult" />
    </output>

  </parameters>
</program_description>
